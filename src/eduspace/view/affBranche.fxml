            <Font name="Roboto Light" size="20.0"/>
         </font>
      </Button>
      <Button layoutX="1014.0" layoutY="25.0" mnemonicParsing="false" onAction="#close" prefHeight="49.0"
              prefWidth="42.0" styleClass="btnClose" text="X" textFill="WHITE" AnchorPane.rightAnchor="20.0"
